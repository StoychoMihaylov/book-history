services:
  angular.app:
    build:
      context: C:\Users\stmih\source\repos\BookHistory
      dockerfile: BookHistory.App/ClientApp/Dockerfile
    container_name: angular.app
    image: bookhistoryapp
    ports:
    - published: 4200
      target: 4200
    - published: 49153
      target: 49153
    restart: on-failure
    volumes:
    - C:\Users\stmih\source\repos\BookHistory\BookHistory.App\ClientApp:/app:rw
  bookhistory.app:
    build:
      args:
        skip_client_build: "true"
      context: C:\Users\stmih\source\repos\BookHistory
      dockerfile: BookHistory.App/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: BookHistory.App
      target: base
    container_name: book.history.api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: bookhistoryapp:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/BookHistory.App.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\stmih\source\repos\BookHistory\BookHistory.App:/app:rw
    - C:\Users\stmih\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\stmih\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\stmih\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\stmih\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\stmih\source\repos\BookHistory:/src:rw
  pgadmin:
    container_name: pgadmin4_container
    environment:
      PGADMIN_DEFAULT_EMAIL: admin@admin.com
      PGADMIN_DEFAULT_PASSWORD: admin
    image: dpage/pgadmin4
    ports:
    - published: 5050
      target: 80
    restart: always
    volumes:
    - C:\Users\stmih\source\repos\BookHistory\src\Tributech.DataSpace.DB\pgadmin_servers.json:/pgadmin4/servers.json:rw
  postgre.server:
    container_name: postgre.book.history.srv
    environment:
      POSTGRES_PASSWORD: admin
      POSTGRES_USER: admin
    image: postgres:latest
    ports:
    - published: 5432
      target: 5432
    restart: on-failure
    volumes:
    - postgresql-db:/var/lib/postgresql/data:rw
version: '3.4'
volumes:
  postgresql-db: {}